                    <div class="contact-form "  >

                        <form method="post" action="{{ path('contact_contactpage') }}" id="contactform" class="contact">
                            <table >
                                <div class="form-group has-feedback">


                                    <label for="name">Nom d'utilisateur*</label>
                                    {{ form_widget(form.name, { 'attr': {'class': 'form-control', 'placeholder': ''|trans } }) }}<br>
                                    {#<input class ="form-control" type="text" id="foodcorner_contactbundle_contact_name" name="foodcorner_contactbundle_contact[name]" required="required" maxlength="255" />#}
                                    <i class="fa fa-user form-control-feedback"></i>
                                </div>
                                <div class="form-group has-feedback">
                                    <label for="email">Email*</label>
                                    {{ form_widget(form.email, { 'attr': {'class': 'form-control', 'placeholder': ''|trans } }) }}<br>
                                    {#<input class="form-control" type="text" id="foodcorner_contactbundle_contact_email" name="foodcorner_contactbundle_contact[email]" required="required" maxlength="255" />#}
                                    <i class="fa fa-envelope form-control-feedback"></i>
                                </div>
                                <div class="form-group has-feedback">
                                    <label for="email">Telephone*</label>
                                    {{ form_widget(form.phone, { 'attr': {'class': 'form-control', 'placeholder': ''|trans } }) }}<br>
                                    {#<input  class ="form-control" type="number" id="foodcorner_contactbundle_contact_phone" name="foodcorner_contactbundle_contact[phone]" required="required" />#}
                                    <i class="fa fa-envelope form-control-feedback"></i>
                                </div>
                                <div class="form-group has-feedback">
                                    <label for="message">Message*</label>
                                    {{ form_widget(form.message, { 'attr': {'class': 'form-control', 'placeholder': ''|trans } }) }}<br>
                                    {#<textarea class="form-control" id="foodcorner_contactbundle_contact_message" name="foodcorner_contactbundle_contact[message]" required="required"></textarea>#}
                                    <i class="fa fa-pencil form-control-feedback"></i>
                                </div>
                            </table>
                            <input type="submit" value="send" id="submit" class="submit btn btn-success">
                            {{ form_end(form) }}
                        </form>


